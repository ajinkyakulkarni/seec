  SEEC_PP_QUOTE(char const *filename; char const *mode; FILE *stream;),
  SEEC_PP_QUOTE(, filename, mode, stream),
  SEEC_PP_QUOTE(char const *, char const *, FILE *))

DETECT_CALL(C, fclose, SEEC_PP_QUOTE(FILE *stream;), SEEC_PP_QUOTE(, stream), SEEC_PP_QUOTE(FILE *))
