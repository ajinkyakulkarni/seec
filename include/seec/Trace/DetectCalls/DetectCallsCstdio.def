DETECT_CALL(C, setvbuf,
  SEEC_PP_QUOTE(FILE *stream; char *buffer; int mode; size_t size;),
  SEEC_PP_QUOTE(, stream, buffer, mode, size),
  SEEC_PP_QUOTE(FILE *, char *, int, size_t))


