#ifndef SEEC_REDIRECT_CALL
#error "RedirectCalls.def included but SEEC_REDIRECT_CALL not defined!"
// #define SEEC_REDIRECT_CALL(NAME, RET_TY, RET, PARAMETERS, ARGUMENTS)
#endif

#ifndef SEEC_REDIRECT_CALL_PARAMETERS
#define SEEC_REDIRECT_CALL_PARAMETERS(...) __VA_ARGS__
#endif

#ifndef SEEC_REDIRECT_CALL_ARGUMENTS
#define SEEC_REDIRECT_CALL_ARGUMENTS(...) __VA_ARGS__
#endif

SEEC_REDIRECT_CALL(exit, void, ,
  SEEC_REDIRECT_CALL_PARAMETERS(, int code),
  SEEC_REDIRECT_CALL_ARGUMENTS(code)
  )

SEEC_REDIRECT_CALL(atexit, int, return,
  SEEC_REDIRECT_CALL_PARAMETERS(, void (*function)(void)),
  SEEC_REDIRECT_CALL_ARGUMENTS(function)
  )

#undef SEEC_REDIRECT_CALL_ARGUMENTS
#undef SEEC_REDIRECT_CALL_PARAMETERS
#undef SEEC_REDIRECT_CALL
