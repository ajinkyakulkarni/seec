SEEC_FUNCTION_HANDLED_FL(tan)
SEEC_FUNCTION_HANDLED_FL(asin)
SEEC_FUNCTION_HANDLED_FL(acos)
SEEC_FUNCTION_HANDLED_FL(atan)
SEEC_FUNCTION_HANDLED_FL(atan2)

SEEC_FUNCTION_HANDLED_FL(sinh)
